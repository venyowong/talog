        {
            "name": "eap",
            "log_type": "raw",
            "log_header": "eap",
            "log_regex": "\\[(?P<time>[^\\]]*)\\] \\[(?P<level>[^\\]]*)\\] (?P<msg>.*)$",
            "fields": [
                {
                    "name": "time",
                    "type": "string"
                },
                {
                    "name": "level",
                    "tag_name": "level",
                    "type": "string"
                },
                {
                    "name": "name",
